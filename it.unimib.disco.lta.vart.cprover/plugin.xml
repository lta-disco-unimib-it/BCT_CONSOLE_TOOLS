<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.cprover.preferences.CProverMainPreferencePage"
            id="org.cprover.plugin.preferences.CProverPreferencePage"
            name="CProver"/>
      <page
            category="org.cprover.plugin.preferences.CProverPreferencePage"
            class="org.cprover.preferences.ToolsPreferencePage"
            id="org.cprover.plugin.preferences.ToolsPreferencePage"
            name="External Tools">
      </page>
      <page
            category="org.cprover.plugin.preferences.CProverPreferencePage"
            class="org.cprover.preferences.SatabsPreferencePage"
            id="org.cprover.plugin.preferences.SatabsPreferencePage"
            name="SATABS"/>
      <page
            category="org.cprover.plugin.preferences.CProverPreferencePage"
            class="org.cprover.preferences.CbmcPreferencePage"
            id="org.cprover.plugin.preferences.CbmcPreferencePage"            
            name="CBMC"/>
      <page
            category="org.cprover.plugin.preferences.CProverPreferencePage"
            class="org.cprover.preferences.HwCbmcPreferencePage"
            id="org.cprover.plugin.preferences.HwCbmcPreferencePage"
            name="HW-CBMC"/>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer class="org.cprover.preferences.PreferenceInitializer"/>
   </extension>
   
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.cprover.launch.LaunchConfigurationDelegate"
            id="org.cprover.plugin.launchConfigurationType"
            modes="run,debug"
            name="CProver">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="org.cprover.launch.VerifyConfigurationTabGroup"
            id="org.cprover.plugin.launch.LaunchConfigurationTabGroup"
            type="org.cprover.plugin.launchConfigurationType">
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.cprover.perspectives.VerifyPerspective"
            icon="icons/logo.ico"
            id="org.cprover.plugin.perspectives.VerifyPerspective"
            name="CProver">
      </perspective>
   </extension>

<extension
      point="org.eclipse.ui.views">
   <category
         id="org.cprover.plugin"
         name="CProver">
   </category>
   <view
         category="org.cprover.plugin"
         class="org.cprover.views.ClaimsView"
         icon="icons/logo.ico"
         id="org.cprover.views.ClaimsView"
         name="Claims">
   </view>
   <view
         class="org.cprover.views.SampleView"
         icon="icons/sample.gif"
         id="org.cprover.plugin.views.SampleView"
         name="Sample View">
   </view>
   <view
         category="org.cprover.plugin"
         class="org.cprover.views.LoopsView"
         icon="icons/logo.ico"
         id="org.cprover.views.LoopsView"
         name="Loops"
         restorable="true">
   </view>
</extension>
<extension
      point="org.eclipse.debug.core.processFactories">
   <processFactory
         class="org.cprover.launch.CProverProcessFactory"
         id="org.cprover.launch.CProverProcessFactory">
   </processFactory>
</extension>

</plugin>
